using System;

namespace Tabuada
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int a, b;
            Console.WriteLine("Digite um n√∫mero para fazermso a tabuada: ");
            a = int.Parse(Console.ReadLine());

            Console.WriteLine($"\nTabuada do {a}:");

            for (b = 0; b <= 10; b++)
            {
                Console.WriteLine($"{a} x {b} = {a * b}");
            }
        }
    }
}
